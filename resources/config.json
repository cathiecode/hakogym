{
  "services": {
    "sleep": {
      "program": "timeout",
      "default_args": ["5"],
      "default_start": false
    }
  },
  "record": {
    "metadata": {
      "schema": {
        "type": "object",
        "properties": {
          "carId": {
            "type": "string"
          },
          "pylonTouchCount": {
            "type": "number"
          },
          "derailmentCount": {
            "type": "number"
          },
          "status": {
            "type": "string",
            "enum": ["DNS", "MC", "DNF"]
          },
          "removed": {
            "type": "boolean"
          },
          "heat": {
            "type": "number"
          }
        },
        "required": ["carId", "pylonTouchCount", "derailmentCount", "removed"]
      },
      "default": {
        "carId": "",
        "pylonTouchCount": 0,
        "derailmentCount": 0,
        "removed": false
      }
    }
  },
  "server": {
    "addr": "[::1]:11000",
    "service_manager_addr": "[::1]:11001"
  }
}
