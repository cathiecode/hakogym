{
  "engines": {
    "node": "18"
  },
  "devDependencies": {
    "@types/jest": "~29.1",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "~5.39",
    "@typescript-eslint/parser": "~5.39",
    "eslint": "~8.24",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.3",
    "husky": "~8.0",
    "jest": "~29.1",
    "lint-staged": "~13.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.7",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "tsutils": "~3.21",
    "typescript": "~4.8"
  },
  "scripts": {
    "start": "node build/index.js",
    "start:watch": "nodemon -w build/ build/index.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "scripts/build.sh && esbuild src/main.ts --bundle --outfile=build/index.js --platform=node",
    "build:watch": "scripts/build.sh && esbuild src/main.ts --bundle --outfile=build/index.js --platform=node --watch",
    "build:release": "npm run clean && esbuild src/main.ts --bundle --outfile=build/index.js --platform=node",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch",
    "watch": "npm-run-all clean build --parallel start:watch build:watch"
  },
  "dependencies": {
    "@apollo/server": "^4.2.1",
    "@as-integrations/fastify": "^1.2.0",
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fastify": "^4.10.2",
    "fastify-socket.io": "^4.0.0",
    "socket.io": "^4.5.4",
    "tslib": "~2.4"
  }
}
